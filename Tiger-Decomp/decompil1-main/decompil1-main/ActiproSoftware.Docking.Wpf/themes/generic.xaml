<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:docking="clr-namespace:ActiproSoftware.Windows.Controls.Docking" xmlns:dockingPrimitives="clr-namespace:ActiproSoftware.Windows.Controls.Docking.Primitives" xmlns:productsDocking="clr-namespace:ActiproSoftware.Products.Docking" xmlns:shared="http://schemas.actiprosoftware.com/winfx/xaml/shared" xmlns:system="clr-namespace:System;assembly=mscorlib" xmlns:themes="http://schemas.actiprosoftware.com/winfx/xaml/themes">
  <ResourceDictionary.MergedDictionaries>
    <themes:SystemThemeCatalogRegistrar />
  </ResourceDictionary.MergedDictionaries>
  <shared:ConditionalConverter x:Key="BooleanToOpacityConverter">
    <shared:ConditionalConverter.TrueValue>
      <system:Double>1</system:Double>
    </shared:ConditionalConverter.TrueValue>
    <shared:ConditionalConverter.FalseValue>
      <system:Double>0</system:Double>
    </shared:ConditionalConverter.FalseValue>
  </shared:ConditionalConverter>
  <BooleanToVisibilityConverter x:Key="BooleanToVisibilityConverter" />
  <shared:CoalesceConverter x:Key="CoalesceConverter" />
  <shared:CornerRadiusConverter x:Key="CornerRadiusConverter" />
  <shared:ConditionalConverter x:Key="ImageSourceVisibilityConverter">
    <shared:ConditionalConverter.TrueValue>
      <Visibility>Visible</Visibility>
    </shared:ConditionalConverter.TrueValue>
    <shared:ConditionalConverter.FalseValue>
      <Visibility>Collapsed</Visibility>
    </shared:ConditionalConverter.FalseValue>
  </shared:ConditionalConverter>
  <shared:ConditionalConverter x:Key="RotationConditionalConverter">
    <shared:ConditionalConverter.FalseValue>
      <RotateTransform Angle="90" />
    </shared:ConditionalConverter.FalseValue>
  </shared:ConditionalConverter>
  <dockingPrimitives:NonUIElementConverter x:Key="NonUIElementConverter" />
  <dockingPrimitives:SideToDockConverter x:Key="SideToDockConverter" />
  <shared:ThicknessConverter x:Key="ThicknessConverter" />
  <dockingPrimitives:TitleConverter x:Key="TitleConverter" />
  <SolidColorBrush x:Key="SplitterHighlightBrush" Color="#80808080" />
  <Style x:Key="NewTabButtonStyle" TargetType="{x:Type dockingPrimitives:NewTabButton}">
    <Setter Property="Background" Value="#00FFFFFF" />
    <Setter Property="BorderBrush" Value="#00FFFFFF" />
    <Setter Property="BorderThickness" Value="0" />
    <Setter Property="Focusable" Value="False" />
    <Setter Property="Foreground" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabControlNewTabButtonForegroundNormalBrushKey}}" />
    <Setter Property="IsTabStop" Value="False" />
    <Setter Property="Margin" Value="1" />
    <Setter Property="MinWidth" Value="18" />
    <Setter Property="MinHeight" Value="18" />
    <Setter Property="Padding" Value="4" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type dockingPrimitives:NewTabButton}">
          <dockingPrimitives:TabItemBorder TabStripPlacement="{TemplateBinding dockingPrimitives:NewTabButton.TabStripPlacement}" BorderThickness="{TemplateBinding Control.BorderThickness}" UntintedBorderBrush="{TemplateBinding Control.BorderBrush}" UntintedBackground="{TemplateBinding Control.Background}" CornerRadius="{TemplateBinding dockingPrimitives:NewTabButton.CornerRadius}">
            <Canvas Margin="{TemplateBinding Control.Padding}" Width="10" Height="10">
              <Rectangle Canvas.Left="4" Width="2" Height="10" Fill="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}" />
              <Rectangle Canvas.Top="4" Width="10" Height="2" Fill="{Binding Path=Foreground, RelativeSource={RelativeSource TemplatedParent}}" />
            </Canvas>
          </dockingPrimitives:TabItemBorder>
          <ControlTemplate.Triggers>
            <Trigger Property="UIElement.IsMouseOver" Value="True">
              <Setter Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundHoverBrushKey}}" Property="Background" />
              <Setter Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundNormalBrushKey}}" Property="Foreground" />
            </Trigger>
            <Trigger Property="IsPressed" Value="True">
              <Setter Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundPressedBrushKey}}" Property="Background" />
              <Setter Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundNormalBrushKey}}" Property="Foreground" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="EmbeddedButtonStyleKey" TargetType="{x:Type ButtonBase}" BasedOn="{StaticResource {x:Static themes:SharedResourceKeys.EmbeddedButtonBaseOverrideStyleKey}}">
    <Setter Property="MinHeight" Value="18" />
    <Setter Property="MinWidth" Value="18" />
    <Setter Property="Padding" Value="1" />
    <Setter Property="FrameworkElement.UseLayoutRounding" Value="True" />
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type ButtonBase}">
          <Border Name="outerBorder" Background="{TemplateBinding Control.Background}" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding Control.BorderThickness}" CornerRadius="{TemplateBinding themes:ThemeProperties.CornerRadius}" SnapsToDevicePixels="true">
            <Grid>
              <Border Name="innerBorder" BorderBrush="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonInnerBorderNormalBrushKey}}" BorderThickness="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBorderNormalThicknessKey}}" CornerRadius="{TemplateBinding themes:ThemeProperties.CornerRadius}" SnapsToDevicePixels="true" />
              <shared:PixelSnapper VerticalRoundMode="CeilingToEven">
                <ContentPresenter Name="presenter" Margin="{TemplateBinding Control.Padding}" HorizontalAlignment="{TemplateBinding Control.HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding Control.VerticalContentAlignment}" SnapsToDevicePixels="{TemplateBinding UIElement.SnapsToDevicePixels}" Opacity="0.75" />
              </shared:PixelSnapper>
            </Grid>
          </Border>
          <ControlTemplate.Triggers>
            <Trigger Property="ToggleButton.IsChecked" Value="true">
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundCheckedNormalBrushKey}}" Property="Border.Background" />
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBorderCheckedNormalBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="innerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonInnerBorderCheckedNormalBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="presenter" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundHoverBrushKey}}" Property="TextBlock.Foreground" />
            </Trigger>
            <Trigger Property="UIElement.IsMouseOver" Value="true">
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundHoverBrushKey}}" Property="Border.Background" />
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBorderHoverBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="innerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonInnerBorderHoverBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="presenter" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundHoverBrushKey}}" Property="TextBlock.Foreground" />
              <Setter TargetName="presenter" Property="UIElement.Opacity" Value="1" />
            </Trigger>
            <Trigger Property="UIElement.IsKeyboardFocused" Value="true">
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundHoverBrushKey}}" Property="Border.Background" />
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBorderHoverBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="innerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonInnerBorderHoverBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="presenter" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundHoverBrushKey}}" Property="TextBlock.Foreground" />
              <Setter TargetName="presenter" Property="UIElement.Opacity" Value="1" />
            </Trigger>
            <MultiTrigger>
              <MultiTrigger.Conditions>
                <Condition Property="UIElement.IsMouseOver" Value="true" />
                <Condition Property="ToggleButton.IsChecked" Value="true" />
              </MultiTrigger.Conditions>
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundCheckedHoverBrushKey}}" Property="Border.Background" />
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBorderCheckedHoverBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="innerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonInnerBorderCheckedHoverBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="presenter" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundHoverBrushKey}}" Property="TextBlock.Foreground" />
              <Setter TargetName="presenter" Property="UIElement.Opacity" Value="1" />
            </MultiTrigger>
            <MultiTrigger>
              <MultiTrigger.Conditions>
                <Condition Property="UIElement.IsKeyboardFocused" Value="true" />
                <Condition Property="ToggleButton.IsChecked" Value="true" />
              </MultiTrigger.Conditions>
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundCheckedHoverBrushKey}}" Property="Border.Background" />
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBorderCheckedHoverBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="innerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonInnerBorderCheckedHoverBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="presenter" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundHoverBrushKey}}" Property="TextBlock.Foreground" />
              <Setter TargetName="presenter" Property="UIElement.Opacity" Value="1" />
            </MultiTrigger>
            <Trigger Property="IsPressed" Value="true">
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundPressedBrushKey}}" Property="Border.Background" />
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBorderPressedBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="innerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonInnerBorderPressedBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="presenter" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundHoverBrushKey}}" Property="TextBlock.Foreground" />
              <Setter TargetName="presenter" Property="UIElement.Opacity" Value="1" />
            </Trigger>
            <Trigger Property="shared:PopupButton.IsPopupOpen" Value="true">
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBackgroundPressedBrushKey}}" Property="Border.Background" />
              <Setter TargetName="outerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonBorderPressedBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="innerBorder" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonInnerBorderPressedBrushKey}}" Property="Border.BorderBrush" />
              <Setter TargetName="presenter" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ToolBarButtonForegroundHoverBrushKey}}" Property="TextBlock.Foreground" />
              <Setter TargetName="presenter" Property="UIElement.Opacity" Value="1" />
            </Trigger>
            <Trigger Property="IsEnabled" Value="false">
              <Setter TargetName="presenter" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowDisabledBrushKey}}" Property="TextBlock.Foreground" />
              <Setter TargetName="presenter" Property="UIElement.Opacity" Value="{Binding Path=(themes:ThemeProperties.DisabledOpacity), RelativeSource={RelativeSource TemplatedParent}}" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="{x:Type dockingPrimitives:NewTabButton}" TargetType="{x:Type dockingPrimitives:NewTabButton}" BasedOn="{StaticResource NewTabButtonStyle}" />
  <Style x:Key="AdvancedTabControlStyle" TargetType="{x:Type docking:AdvancedTabControl}">
    <Setter Property="Background" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabControlBackgroundNormalBrushKey}}" />
    <Setter Property="BorderBrush" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderNormalBrushKey}}" />
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="docking:AdvancedTabControl.IsActive" Value="{Binding Path=IsKeyboardFocusWithin, RelativeSource={RelativeSource Self}}" />
    <Setter Property="docking:AdvancedTabControl.TabBackground" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBackgroundNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBackgroundActiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBackgroundSelectedNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBackgroundInactiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBackgroundSelectedNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBackgroundPointerOver" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBackgroundHoverBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBackgroundPreview" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBackgroundNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBackgroundPreviewActiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBackgroundSelectedNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBackgroundPreviewPointerOver" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBackgroundHoverBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBorderBrush" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBorderBrushActiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderSelectedNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBorderBrushInactiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderSelectedNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBorderBrushPointerOver" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderHoverBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBorderBrushPreview" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBorderBrushPreviewActiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderSelectedNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabBorderBrushPreviewPointerOver" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderHoverBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabCornerRadius" Value="{DynamicResource {x:Static themes:AssetResourceKeys.TabItemBorderNormalCornerRadiusKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabForeground" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabForegroundActiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabForegroundInactiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabForegroundPointerOver" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabForegroundPreview" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabForegroundPreviewPointerOver" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabForegroundPreviewActiveSelected" Value="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowNormalBrushKey}}" />
    <Setter Property="docking:AdvancedTabControl.EmbeddedButtonStyle" Value="{StaticResource EmbeddedButtonStyleKey}" />
    <Setter Property="ItemsPanel">
      <Setter.Value>
        <ItemsPanelTemplate>
          <dockingPrimitives:AdvancedTabPanel />
        </ItemsPanelTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="docking:AdvancedTabControl.MenuButtonContentTemplate" Value="{DynamicResource {x:Static themes:SharedResourceKeys.DropDownArrowGlyphTemplateKey}}" />
    <Setter Property="docking:AdvancedTabControl.OverflowMenuButtonContentTemplate" Value="{DynamicResource {x:Static themes:SharedResourceKeys.OverflowDropDownArrowGlyphTemplateKey}}" />
    <Setter Property="docking:AdvancedTabControl.ScrollBackwardButtonContentTemplate" Value="{DynamicResource {x:Static themes:SharedResourceKeys.LargeLeftArrowGlyphTemplateKey}}" />
    <Setter Property="docking:AdvancedTabControl.ScrollForwardButtonContentTemplate" Value="{DynamicResource {x:Static themes:SharedResourceKeys.LargeRightArrowGlyphTemplateKey}}" />
    <Setter Property="docking:AdvancedTabControl.TabEmbeddedButtonStyle">
      <Setter.Value>
        <Style TargetType="{x:Type ButtonBase}" BasedOn="{StaticResource EmbeddedButtonStyleKey}">
          <Setter Property="Margin" Value="0,2,2,2" />
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="Template">
      <Setter.Value>
        <ControlTemplate TargetType="{x:Type docking:AdvancedTabControl}">
          <Grid KeyboardNavigation.TabNavigation="Local">
            <Grid.RowDefinitions>
              <RowDefinition Name="RowDef0" Height="*" />
              <RowDefinition Name="RowDef1" Height="Auto" />
            </Grid.RowDefinitions>
            <Grid.ColumnDefinitions>
              <ColumnDefinition Name="ColDef0" Width="*" />
              <ColumnDefinition Name="ColDef1" Width="Auto" />
            </Grid.ColumnDefinitions>
            <Rectangle Name="HeaderBackground" Grid.Row="1" Grid.ColumnSpan="2" Fill="{TemplateBinding docking:AdvancedTabControl.HeaderBackground}" />
            <Grid Name="HeaderPanel" Row="1" ZIndex="1" ClipToBounds="True" KeyboardNavigation.TabNavigation="Local" KeyboardNavigation.TabIndex="1">
              <dockingPrimitives:NewTabButton x:Name="PART_NewTabButton" Panel.ZIndex="2" HorizontalAlignment="Left" Style="{TemplateBinding docking:AdvancedTabControl.NewTabButtonStyle}" TabStripPlacement="{TemplateBinding TabControl.TabStripPlacement}" IsContentHorizontal="{TemplateBinding docking:AdvancedTabControl.IsTabContentHorizontal}" Padding="{TemplateBinding docking:AdvancedTabControl.TabPadding}" CornerRadius="{TemplateBinding docking:AdvancedTabControl.TabCornerRadius}" Visibility="{Binding Path=HasNewTabButton, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" Command="{Binding Path=RequestNewTabCommand, RelativeSource={RelativeSource TemplatedParent}}">
                <TextBlock Text="+" Opacity="0" />
              </dockingPrimitives:NewTabButton>
              <ItemsPresenter />
            </Grid>
            <StackPanel Name="ButtonsPanel" Grid.Row="1" Grid.Column="1" ZIndex="3" Orientation="Horizontal" KeyboardNavigation.TabIndex="2">
              <RepeatButton Name="ScrollBackwardButton" ContentTemplate="{TemplateBinding docking:AdvancedTabControl.ScrollBackwardButtonContentTemplate}" Style="{TemplateBinding docking:AdvancedTabControl.EmbeddedButtonStyle}" AutomationProperties.Name="Scroll Backward" Visibility="{Binding Path=AreScrollButtonsVisible, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" Command="{Binding Path=ScrollBackwardCommand, RelativeSource={RelativeSource TemplatedParent}}" ToolTip="{productsDocking:SR UIAdvancedTabControlScrollBackwardButtonToolTip}" />
              <RepeatButton Name="ScrollForwardButton" ContentTemplate="{TemplateBinding docking:AdvancedTabControl.ScrollForwardButtonContentTemplate}" Style="{TemplateBinding docking:AdvancedTabControl.EmbeddedButtonStyle}" AutomationProperties.Name="Scroll Forward" Visibility="{Binding Path=AreScrollButtonsVisible, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" Command="{Binding Path=ScrollForwardCommand, RelativeSource={RelativeSource TemplatedParent}}" ToolTip="{productsDocking:SR UIAdvancedTabControlScrollForwardButtonToolTip}" />
              <shared:PopupButton Name="PART_MenuButton" ContentTemplate="{TemplateBinding docking:AdvancedTabControl.MenuButtonContentTemplate}" Style="{TemplateBinding docking:AdvancedTabControl.EmbeddedButtonStyle}" ClickMode="Press" AutomationProperties.Name="Menu" Visibility="{Binding Path=IsMenuButtonVisible, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
            </StackPanel>
            <Border Name="HighlightBorder" Grid.ColumnSpan="2" Margin="0,0,0,-1" BorderBrush="{TemplateBinding docking:AdvancedTabControl.HighlightBrushInactive}" BorderThickness="{TemplateBinding docking:AdvancedTabControl.RotatedHighlightThickness}" KeyboardNavigation.TabNavigation="Local" KeyboardNavigation.DirectionalNavigation="Contained" KeyboardNavigation.TabIndex="3">
              <Border Name="ContentBorder" Background="{TemplateBinding Control.Background}" BorderBrush="{TemplateBinding Control.BorderBrush}" BorderThickness="{TemplateBinding docking:AdvancedTabControl.RotatedBorderThickness}">
                <ContentPresenter Name="PART_SelectedContentHost" Margin="{TemplateBinding docking:AdvancedTabControl.RotatedPadding}" Content="{TemplateBinding TabControl.SelectedContent}" ContentTemplate="{TemplateBinding TabControl.SelectedContentTemplate}" ContentTemplateSelector="{TemplateBinding TabControl.SelectedContentTemplateSelector}" />
              </Border>
            </Border>
          </Grid>
          <ControlTemplate.Triggers>
            <Trigger Property="docking:AdvancedTabControl.IsActive" Value="True">
              <Setter TargetName="HighlightBorder" Property="Border.BorderBrush" Value="{Binding Path=HighlightBrushActive, RelativeSource={RelativeSource TemplatedParent}}" />
            </Trigger>
            <Trigger Property="docking:AdvancedTabControl.IsOverflowed" Value="True">
              <Setter TargetName="PART_MenuButton" Property="ContentControl.ContentTemplate" Value="{Binding Path=OverflowMenuButtonContentTemplate, RelativeSource={RelativeSource TemplatedParent}}" />
            </Trigger>
            <Trigger Property="TabControl.TabStripPlacement" Value="Top">
              <Setter TargetName="RowDef0" Property="RowDefinition.Height" Value="Auto" />
              <Setter TargetName="RowDef1" Property="RowDefinition.Height" Value="*" />
              <Setter TargetName="HeaderBackground" Property="Grid.Row" Value="0" />
              <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="0" />
              <Setter TargetName="ButtonsPanel" Property="Grid.Row" Value="0" />
              <Setter TargetName="HighlightBorder" Property="Grid.Row" Value="1" />
              <Setter TargetName="HighlightBorder" Property="Margin" Value="0,-1,0,0" />
            </Trigger>
            <Trigger Property="TabControl.TabStripPlacement" Value="Left">
              <Setter TargetName="ColDef0" Property="ColumnDefinition.Width" Value="Auto" />
              <Setter TargetName="ColDef1" Property="ColumnDefinition.Width" Value="*" />
              <Setter TargetName="PART_NewTabButton" Property="HorizontalAlignment" Value="Stretch" />
              <Setter TargetName="PART_NewTabButton" Property="VerticalAlignment" Value="Top" />
              <Setter TargetName="ButtonsPanel" Property="FrameworkElement.LayoutTransform">
                <Setter.Value>
                  <RotateTransform Angle="90" />
                </Setter.Value>
              </Setter>
              <Setter TargetName="HeaderBackground" Property="Grid.Row" Value="0" />
              <Setter TargetName="HeaderBackground" Property="Grid.RowSpan" Value="2" />
              <Setter TargetName="HeaderBackground" Property="Grid.ColumnSpan" Value="1" />
              <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="0" />
              <Setter TargetName="ButtonsPanel" Property="Grid.Column" Value="0" />
              <Setter TargetName="HighlightBorder" Property="Grid.Column" Value="1" />
              <Setter TargetName="HighlightBorder" Property="Grid.ColumnSpan" Value="1" />
              <Setter TargetName="HighlightBorder" Property="Grid.RowSpan" Value="2" />
              <Setter TargetName="HighlightBorder" Property="Margin" Value="-1,0,0,0" />
            </Trigger>
            <Trigger Property="TabControl.TabStripPlacement" Value="Right">
              <Setter TargetName="PART_NewTabButton" Property="HorizontalAlignment" Value="Stretch" />
              <Setter TargetName="PART_NewTabButton" Property="VerticalAlignment" Value="Top" />
              <Setter TargetName="ButtonsPanel" Property="FrameworkElement.LayoutTransform">
                <Setter.Value>
                  <RotateTransform Angle="90" />
                </Setter.Value>
              </Setter>
              <Setter TargetName="HeaderBackground" Property="Grid.Row" Value="0" />
              <Setter TargetName="HeaderBackground" Property="Grid.RowSpan" Value="2" />
              <Setter TargetName="HeaderBackground" Property="Grid.Column" Value="1" />
              <Setter TargetName="HeaderBackground" Property="Grid.ColumnSpan" Value="1" />
              <Setter TargetName="HeaderPanel" Property="Grid.Column" Value="1" />
              <Setter TargetName="HeaderPanel" Property="Grid.Row" Value="0" />
              <Setter TargetName="HighlightBorder" Property="Grid.ColumnSpan" Value="1" />
              <Setter TargetName="HighlightBorder" Property="Grid.RowSpan" Value="2" />
              <Setter TargetName="HighlightBorder" Property="Margin" Value="0,0,-1,0" />
            </Trigger>
            <Trigger Property="docking:AdvancedTabControl.IsTabStripVisible" Value="False">
              <Setter TargetName="HeaderBackground" Property="Visibility" Value="Collapsed" />
              <Setter TargetName="HeaderPanel" Property="UIElement.Opacity" Value="0" />
              <Setter TargetName="HeaderPanel" Property="Width" Value="1" />
              <Setter TargetName="HeaderPanel" Property="Height" Value="1" />
              <Setter TargetName="HeaderPanel" Property="Margin" Value="-1,-1,0,0" />
              <Setter TargetName="ButtonsPanel" Property="Visibility" Value="Collapsed" />
              <Setter TargetName="HighlightBorder" Property="Margin" Value="0" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
  <Style x:Key="{x:Type docking:AdvancedTabControl}" TargetType="{x:Type docking:AdvancedTabControl}" BasedOn="{StaticResource AdvancedTabControlStyle}" />
  <Style x:Key="AdvancedTabItemStyle" TargetType="{x:Type docking:AdvancedTabItem}">
    <Setter Property="BorderThickness" Value="1" />
    <Setter Property="docking:AdvancedTabItem.CloseButtonContentTemplate" Value="{DynamicResource {x:Static themes:SharedResourceKeys.CloseGlyphTemplateKey}}" />
    <Setter Property="FrameworkElement.FocusVisualStyle">
      <Setter.Value>
        <Style>
          <Setter Property="Control.Template">
            <Setter.Value>
              <ControlTemplate>
                <Rectangle Margin="3" StrokeThickness="1" Stroke="{DynamicResource {x:Static themes:AssetResourceKeys.ContainerForegroundLowDisabledBrushKey}}" StrokeDashArray="1 2" SnapsToDevicePixels="true" />
              </ControlTemplate>
            </Setter.Value>
          </Setter>
        </Style>
      </Setter.Value>
    </Setter>
    <Setter Property="HorizontalContentAlignment" Value="Stretch" />
    <Setter Property="docking:AdvancedTabItem.PinButtonContentTemplate" Value="{DynamicResource {x:Static themes:SharedResourceKeys.PinGlyphTemplateKey}}" />
    <Setter Property="docking:AdvancedTabItem.PromoteButtonContentTemplate" Value="{DynamicResource {x:Static themes:SharedResourceKeys.PromoteTabGlyphTemplateKey}}" />
    <Setter Property="docking:AdvancedTabItem.ReadOnlyContextContentTemplate">
      <Setter.Value>
        <DataTemplate>
          <Canvas Margin="4,0,4,1" Width="7" Height="9" Opacity="0.75">
            <Path Width="7" Height="4" Canvas.Left="1" StrokeThickness="1.0" Data="M0.5,4 L0.5,2 A0.75,1 90 0 1 4.5,2L4.5,4" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
            <Path Width="7" Height="5" Canvas.Top="4" Data="M0,1 L1,0 L6,0 L7,1 L7,5 L0,5 M3,1 L4,1 L4,3 L3,3" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
          </Canvas>
        </DataTemplate>
      </Setter.Value>
    </Setter>
    <Setter Property="UIElement.RenderTransformOrigin" Value="0,0" />
    <Setter Property="docking:AdvancedTabItem.UnpinButtonContentTemplate" Value="{DynamicResource {x:Static themes:SharedResourceKeys.UnpinGlyphTemplateKey}}" />
    <Setter Property="VerticalContentAlignment" Value="Stretch" />
    <Setter Property="Template">
      <Setter.Value>
