              <Border Name="innerBorderTop" Grid.Row="0" Grid.ColumnSpan="3" Background="{DynamicResource {x:Static themes:AssetResourceKeys.WindowInnerBorderTopInactiveBrushKey}}" Height="{Binding Path=BorderThickness.Top, RelativeSource={RelativeSource TemplatedParent}}" CornerRadius="{Binding Path=CornerRadius, ConverterParameter=Top, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource CornerRadiusConverter}}" />
              <Rectangle Name="innerBorderLeft" Grid.Row="1" Grid.RowSpan="2" Grid.Column="0" Fill="{DynamicResource {x:Static themes:AssetResourceKeys.WindowInnerBorderLeftInactiveBrushKey}}" Width="{Binding Path=BorderThickness.Left, RelativeSource={RelativeSource TemplatedParent}}" />
              <Rectangle Name="innerBorderRight" Grid.Row="1" Grid.RowSpan="2" Grid.Column="2" Fill="{DynamicResource {x:Static themes:AssetResourceKeys.WindowInnerBorderRightInactiveBrushKey}}" Width="{Binding Path=BorderThickness.Right, RelativeSource={RelativeSource TemplatedParent}}" />
              <Border Name="titleBarBackground" Grid.Row="1" Grid.Column="1" Margin="0,0,0,-1" Background="{DynamicResource {x:Static themes:AssetResourceKeys.WindowTitleBarBackgroundInactiveBrushKey}}" Visibility="{Binding Path=HasTitleBar, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
              <Border Name="innerBorderBottom" Grid.Row="3" Grid.ColumnSpan="3" Background="{DynamicResource {x:Static themes:AssetResourceKeys.WindowInnerBorderBottomInactiveBrushKey}}" Height="{Binding Path=BorderThickness.Bottom, RelativeSource={RelativeSource TemplatedParent}}" CornerRadius="{Binding Path=CornerRadius, ConverterParameter=Bottom, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource CornerRadiusConverter}}" />
              <Border Name="outerBorder" Grid.RowSpan="4" Grid.ColumnSpan="3" BorderThickness="1" BorderBrush="{TemplateBinding Control.BorderBrush}" CornerRadius="{TemplateBinding docking:WindowControl.CornerRadius}" />
              <Border Name="PART_TitleBar" Grid.Row="1" Grid.Column="1" Panel.ZIndex="1" ClipToBounds="True" Background="#00FFFFFF" SnapsToDevicePixels="True" Visibility="{Binding Path=HasTitleBar, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}">
                <dockingPrimitives:TitleBarPanel>
                  <Grid dockingPrimitives:TitleBarPanel.IsHeader="True">
                    <TextBlock Name="titleBarText" Margin="8,4" Text="{TemplateBinding docking:WindowControl.Title}" TextTrimming="CharacterEllipsis" TextWrapping="NoWrap" VerticalAlignment="Center" FontWeight="{TemplateBinding docking:WindowControl.TitleBarFontWeight}" Foreground="{DynamicResource {x:Static themes:AssetResourceKeys.WindowTitleBarForegroundInactiveBrushKey}}" />
                  </Grid>
                  <shared:DynamicImage Name="PART_Icon" Margin="8,6,0,6" Source="{TemplateBinding docking:WindowControl.Icon}" Height="16" Width="16" VerticalAlignment="Center" dockingPrimitives:TitleBarPanel.IsIcon="True" Visibility="{Binding Path=HasIcon, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                  <shared:PixelSnapper VerticalRoundMode="CeilingToEven" dockingPrimitives:TitleBarPanel.IsContextContent="True">
                    <ContentControl Name="ContextContentControl" IsTabStop="False" Content="{TemplateBinding docking:WindowControl.ContextContent}" ContentTemplate="{TemplateBinding docking:WindowControl.ContextContentTemplate}" ContentTemplateSelector="{TemplateBinding docking:WindowControl.ContextContentTemplateSelector}" VerticalAlignment="Center" />
                  </shared:PixelSnapper>
                  <shared:PixelSnapper VerticalRoundMode="CeilingToEven" dockingPrimitives:TitleBarPanel.IsReadOnlyContextContent="True">
                    <ContentControl Name="ReadOnlyContextContentControl" IsTabStop="False" Content="" ContentTemplate="{TemplateBinding docking:WindowControl.ReadOnlyContextContentTemplate}" ContentTemplateSelector="{TemplateBinding docking:WindowControl.ContextContentTemplateSelector}" VerticalAlignment="Center" Visibility="{Binding Path=IsReadOnly, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" />
                  </shared:PixelSnapper>
                  <Button Name="minimizeButton" AutomationProperties.Name="Minimize" Style="{DynamicResource {x:Static themes:SharedResourceKeys.WindowTitleBarButtonBaseStyleKey}}" ContentTemplate="{DynamicResource {x:Static themes:SharedResourceKeys.WindowTitleBarButtonMinimizeGlyphTemplateKey}}" themes:ThemeProperties.IsActive="{TemplateBinding docking:WindowControl.IsActive}" Visibility="{Binding Path=IsMinimizeButtonVisible, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" Command="{Binding Path=MinimizeCommand, RelativeSource={RelativeSource TemplatedParent}}" ToolTip="{productsDocking:SR UIWindowControlMinimizeButtonToolTip}" />
                  <Button Name="restoreButton" AutomationProperties.Name="Restore" Style="{DynamicResource {x:Static themes:SharedResourceKeys.WindowTitleBarButtonBaseStyleKey}}" ContentTemplate="{DynamicResource {x:Static themes:SharedResourceKeys.WindowTitleBarButtonRestoreGlyphTemplateKey}}" themes:ThemeProperties.IsActive="{TemplateBinding docking:WindowControl.IsActive}" Visibility="{Binding Path=IsRestoreButtonVisible, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" Command="{Binding Path=RestoreCommand, RelativeSource={RelativeSource TemplatedParent}}" ToolTip="{productsDocking:SR UIWindowControlRestoreButtonToolTip}" />
                  <Button Name="maximizeButton" AutomationProperties.Name="Maximize" Style="{DynamicResource {x:Static themes:SharedResourceKeys.WindowTitleBarButtonBaseStyleKey}}" ContentTemplate="{DynamicResource {x:Static themes:SharedResourceKeys.WindowTitleBarButtonMaximizeGlyphTemplateKey}}" themes:ThemeProperties.IsActive="{TemplateBinding docking:WindowControl.IsActive}" Visibility="{Binding Path=IsMaximizeButtonVisible, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" Command="{Binding Path=MaximizeCommand, RelativeSource={RelativeSource TemplatedParent}}" ToolTip="{productsDocking:SR UIWindowControlMaximizeButtonToolTip}" />
                  <Button Name="closeButton" AutomationProperties.Name="Close" Style="{DynamicResource {x:Static themes:SharedResourceKeys.CloseWindowTitleBarButtonBaseStyleKey}}" ContentTemplate="{DynamicResource {x:Static themes:SharedResourceKeys.WindowTitleBarButtonCloseGlyphTemplateKey}}" themes:ThemeProperties.IsActive="{TemplateBinding docking:WindowControl.IsActive}" Visibility="{Binding Path=IsCloseButtonVisible, RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource BooleanToVisibilityConverter}}" Command="{Binding Path=CloseCommand, RelativeSource={RelativeSource TemplatedParent}}" ToolTip="{productsDocking:SR UIWindowControlCloseButtonToolTip}" />
                </dockingPrimitives:TitleBarPanel>
              </Border>
              <Border Name="contentBorder" Grid.Row="2" Grid.Column="1" Background="{TemplateBinding Control.Background}" Padding="{TemplateBinding Control.Padding}" ClipToBounds="True">
                <ContentPresenter ContentTemplateSelector="{TemplateBinding ContentControl.ContentTemplateSelector}" ContentTemplate="{TemplateBinding ContentControl.ContentTemplate}" Content="{TemplateBinding ContentControl.Content}" />
              </Border>
            </Grid>
          </shared:ShadowChrome>
          <ControlTemplate.Triggers>
            <Trigger Property="docking:WindowControl.WindowState" Value="Minimized">
              <Setter TargetName="contentBorder" Property="Visibility" Value="Collapsed" />
            </Trigger>
            <Trigger Property="docking:WindowControl.WindowState" Value="Maximized">
              <Setter TargetName="outerBorder" Property="Border.BorderThickness" Value="0" />
              <Setter TargetName="innerBorderTop" Property="Height" Value="0" />
              <Setter TargetName="innerBorderLeft" Property="Width" Value="0" />
              <Setter TargetName="innerBorderRight" Property="Width" Value="0" />
              <Setter TargetName="innerBorderBottom" Property="Height" Value="0" />
            </Trigger>
            <MultiTrigger>
              <MultiTrigger.Conditions>
                <Condition Property="docking:WindowControl.IsMaximizedFrameVisible" Value="False" />
                <Condition Property="docking:WindowControl.WindowState" Value="Maximized" />
              </MultiTrigger.Conditions>
              <Setter TargetName="shadow" Property="shared:ShadowChrome.Elevation" Value="0" />
              <Setter TargetName="titleBarBackground" Property="Visibility" Value="Collapsed" />
              <Setter TargetName="PART_TitleBar" Property="Visibility" Value="Collapsed" />
            </MultiTrigger>
            <Trigger Property="docking:WindowControl.HasDropShadow" Value="False">
              <Setter TargetName="shadow" Property="shared:ShadowChrome.Elevation" Value="0" />
            </Trigger>
            <Trigger Property="BorderThickness" Value="0">
              <Setter TargetName="outerBorder" Property="Border.BorderThickness" Value="0" />
            </Trigger>
            <Trigger Property="docking:WindowControl.IsActive" Value="True">
              <Setter Value="{DynamicResource {x:Static themes:AssetResourceKeys.WindowBorderActiveBrushKey}}" Property="BorderBrush" />
              <Setter TargetName="innerBorderTop" Value="{DynamicResource {x:Static themes:AssetResourceKeys.WindowInnerBorderTopActiveBrushKey}}" Property="Border.Background" />
              <Setter TargetName="titleBarBackground" Value="{DynamicResource {x:Static themes:AssetResourceKeys.WindowTitleBarBackgroundActiveBrushKey}}" Property="Border.Background" />
              <Setter TargetName="titleBarText" Value="{DynamicResource {x:Static themes:AssetResourceKeys.WindowTitleBarForegroundActiveBrushKey}}" Property="TextBlock.Foreground" />
              <Setter TargetName="innerBorderLeft" Value="{DynamicResource {x:Static themes:AssetResourceKeys.WindowInnerBorderLeftActiveBrushKey}}" Property="Shape.Fill" />
              <Setter TargetName="innerBorderRight" Value="{DynamicResource {x:Static themes:AssetResourceKeys.WindowInnerBorderRightActiveBrushKey}}" Property="Shape.Fill" />
              <Setter TargetName="innerBorderBottom" Value="{DynamicResource {x:Static themes:AssetResourceKeys.WindowInnerBorderBottomActiveBrushKey}}" Property="Border.Background" />
            </Trigger>
          </ControlTemplate.Triggers>
        </ControlTemplate>
      </Setter.Value>
    </Setter>
  </Style>
