<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:themes="clr-namespace:ActiproSoftware.Windows.Themes">
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.ToolWindowTitleBarButtonCloseGlyphTemplateKey}">
    <Canvas Margin="1" Width="7" Height="7">
      <Path StrokeThickness="0.8" Width="7" Height="7" Data="M0.5,0.5 L1.5,0.5 L3.5,2.5 L5.5,0.5 L6.5,0.5 L6.5,1.5 L4.5,3.5 L6.5,5.5 L6.5,6.5 L5.5,6.5 L3.5,4.5 L1.5,6.5 L0.5,6.5 L0.5,5.5 L2.5,3.5 L0.5,1.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.ToolWindowTitleBarButtonMaximizeGlyphTemplateKey}">
    <Canvas Margin="1" Width="7" Height="7">
      <Path Data="M0,0 L7,0 L7,2 L0,2" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
      <Path Canvas.Top="1" StrokeThickness="1" Data="M0.5,0 L0.5,5.5 L6.5,5.5 L6.5,0" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.ToolWindowTitleBarButtonMinimizeGlyphTemplateKey}">
    <Canvas Margin="1" Width="7" Height="7">
      <Path Canvas.Left="1" Canvas.Top="5" Data="M0,0 L5,0 L5,2 L0,2" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.ToolWindowTitleBarButtonRestoreGlyphTemplateKey}">
    <Canvas Margin="1" Width="7" Height="7">
      <Path Data="M0,2.5 L4,2.5 L4,4.5 L0,4.5 M3,0 L7,0 L7,2 L3,2" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
      <Path StrokeThickness="1" Data="M0.5,4 L0.5,6.5 L3.5,6.5 L3.5,4 M6.5,1 L6.5,4.5 L4.5,4.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.WindowTitleBarButtonBackGlyphTemplateKey}">
    <Canvas Margin="6,0" Width="16" Height="16">
      <Path Canvas.Left="2" Canvas.Top="3" StrokeThickness="1" Data="M0.5,5.5 L12,5.5 M6,0 L0.5,5.5 L6,11" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.WindowTitleBarButtonCloseGlyphTemplateKey}">
    <Canvas Margin="6,0" Width="16" Height="16">
      <Path Canvas.Left="3" Canvas.Top="3" StrokeThickness="1" Data="M0,0 L10,10 M0,10 L10,0" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.WindowTitleBarButtonHelpGlyphTemplateKey}">
    <Canvas Margin="6,0" Width="16" Height="16">
      <Path StrokeThickness="1" Data="M5.5,6 C5.5,4.5 6.5,3.5 8,3.5 C9.5,3.5 10.5,4 10.5,5.5 C10.5,7 7.5,8.5 7.5,10.5 M7,12.75 L8.5,12.75 M7,13.25 L8.5,13.25" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.WindowTitleBarButtonMaximizeGlyphTemplateKey}">
    <Canvas Margin="6,0" Width="16" Height="16">
      <Path Canvas.Left="3" Canvas.Top="3" StrokeThickness="1" Data="M0.5,0.5 L0.5,9.5 L9.5,9.5 L9.5,0.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.WindowTitleBarButtonMenuBarsGlyphTemplateKey}">
    <Canvas Margin="6,0" Width="16" Height="16">
      <Path Canvas.Left="3" Canvas.Top="4" Data="M0,0 L10,0 L10,1 L0,1 M0,4 L10,4 L10,5 L0,5 M0,8 L10,8 L10,9 L0,9" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.WindowTitleBarButtonMinimizeGlyphTemplateKey}">
    <Canvas Margin="6,0" Width="16" Height="16">
      <Path Canvas.Left="3" Canvas.Top="8" Data="M0,0 L10,0 L10,1 L0,1" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.WindowTitleBarButtonRestoreGlyphTemplateKey}">
    <Canvas Margin="6,0" Width="16" Height="16">
      <Path Canvas.Left="3" Canvas.Top="3" StrokeThickness="1" Data="M0.5,2.5 L0.5,9.5 L7.5,9.5 L7.5,2.5 M2.5,2.5 L2.5,0.5 L9.5,0.5 L9.5,7.5 L7.5,7.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
</ResourceDictionary>