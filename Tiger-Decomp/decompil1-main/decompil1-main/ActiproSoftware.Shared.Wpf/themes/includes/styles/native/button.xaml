            <Trigger Property="UIElement.IsMouseOver" Value="True">
              <Setter Value="{DynamicResource {x:Static themes:AssetResourceKeys.ButtonForegroundHoverBrushKey}}" Property="Foreground" />
              <Setter TargetName="chrome" Property="controlsPrimitives:BulletChrome.State" Value="Hover" />
