<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:system="clr-namespace:System;assembly=mscorlib" xmlns:themes="clr-namespace:ActiproSoftware.Windows.Themes">
  <system:Double x:Key="GlyphStrokeThickness">1.25</system:Double>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.CloseGlyphTemplateKey}">
    <Canvas Width="9" Height="9">
      <Path Data="M0,0 L9,9 M0,9 L9,0" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.DownArrowGlyphTemplateKey}">
    <Canvas Width="7" Height="5" SnapsToDevicePixels="False">
      <Path Canvas.Top="1" Data="M0,0 L3.5,3.5 L7,0" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.DropDownArrowGlyphTemplateKey}">
    <Canvas Width="7" Height="5" SnapsToDevicePixels="False">
      <Path Canvas.Top="1" Data="M0,0 L3.5,3.5 L7,0" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.ExternalWindowGlyphTemplateKey}">
    <Canvas Width="14" Height="14">
      <Path StrokeThickness="1" Data="M7.5,2.5 L0.5,2.5 L0.5,13.5 L11.5,13.5 L11.5,6.5 M5.5,8.5 L13.5,0.5 M8.5,0.5 L13.5,0.5 L13.5,5.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LargeDownArrowGlyphTemplateKey}">
    <Canvas Width="9" Height="5" SnapsToDevicePixels="False">
      <Path Data="M0,0 L4.5,4.5 L9,0" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LargeLeftArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="9" SnapsToDevicePixels="False">
      <Path Data="M5,0 L0.5,4.5 L5,9" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LargeRightArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="9" SnapsToDevicePixels="False">
      <Path Data="M0,0 L4.5,4.5 L0,9" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LargeUpArrowGlyphTemplateKey}">
    <Canvas Width="9" Height="5" SnapsToDevicePixels="False">
      <Path Data="M0,5 L4.5,0.5 L9,5" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LeftArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="7" SnapsToDevicePixels="False">
      <Path Data="M4,0 L0.5,3.5 L4,7" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.MaximizeGlyphTemplateKey}">
    <Canvas Width="9" Height="9">
      <Path Data="M0,0 L9,0 L9,3 L0,3" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
      <Path Canvas.Top="3" StrokeThickness="1" Data="M0.5,0 L0.5,5.5 L8.5,5.5 L8.5,0" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.MenuCheckGlyphTemplateKey}">
    <Canvas Width="12" Height="12">
      <Path Data="M0,6.5 L1,5.5 L4,8.5 L11,1.5 L12,2.5 L4,10.5" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.MenuCircleGlyphTemplateKey}">
    <Ellipse Width="8" Height="8" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.MenuIndeterminateCheckGlyphTemplateKey}">
    <Canvas Width="8" Height="8">
      <Path Data="M0,0 L8,0 L8,8 L0,8" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.MenuPopupGlyphTemplateKey}">
    <Canvas Width="5" Height="9" SnapsToDevicePixels="False">
      <Path Data="M0,0 L4.5,4.5 L0,9" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.MinimizeGlyphTemplateKey}">
    <Canvas Width="9" Height="1">
      <Path StrokeThickness="1" Data="M0,0.5 L9,0.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.MinusGlyphTemplateKey}">
    <Canvas Width="11" Height="11">
      <Path StrokeThickness="1" Data="M0,5.5 L11,5.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.OverflowDropDownArrowGlyphTemplateKey}">
    <Canvas Width="7" Height="7">
      <Path Data="M0,0 L7,0 L7,1 L0,1" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
      <Path Canvas.Top="4" Data="M0,0 L3.5,3.5 L7,0" SnapsToDevicePixels="False" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.OverflowRightArrowGlyphTemplateKey}">
    <Canvas Width="9" Height="7">
      <Path StrokeThickness="1" Data="M0.5,0.5 L3.5,3.5 L0.5,6.5 M5.5,0.5 L8.5,3.5 L5.5,6.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.PinGlyphTemplateKey}">
    <Canvas Width="11" Height="7">
      <Path Data="F1 M4,0 L4,1 L10,1 L10,6 L4,6 L4,4 L9,4 L9,2 L4,2 L4,4 L4,7 L3,7 L3,4 L0,4 L0,3 L3,3 L3,0" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.PlusGlyphTemplateKey}">
    <Canvas Width="11" Height="11">
      <Path StrokeThickness="1" Data="M0,5.5 L11,5.5 M5.5,0 L5.5,11" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.PromoteTabGlyphTemplateKey}">
    <Canvas Width="11" Height="7">
      <Path Canvas.Left="2" Canvas.Top="2" Data="M0,5 L0,4 L1,4 L1,1 L2,0 L7,0 L8,1 L8,4 L9,4 L9,5" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
      <Path Canvas.Left="1" Data="M0.5,0.5 L2.5,2.5 M0.5,2.5 L2.5,2.5 L2.5,0.5" StrokeStartLineCap="Square" StrokeEndLineCap="Square" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.RestoreGlyphTemplateKey}">
    <Canvas Width="9" Height="9">
      <Path Data="M0,3 L6,3 L6,5 L0,5 M3,0 L9,0 L9,2 L3,2" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
      <Path StrokeThickness="1" Data="M0.5,5 L0.5,8.5 L5.5,8.5 L5.5,5 M8.5,2 L8.5,5.5 L6,5.5 M3.5,4 L3.5,2" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.RightArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="7" SnapsToDevicePixels="False">
      <Path Canvas.Left="1" Data="M0,0 L3.5,3.5 L0,7" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallDownArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="4" SnapsToDevicePixels="False">
      <Path Canvas.Top="1" Data="M0,0 L2.5,2.5 L5,0" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallDropDownArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="4" SnapsToDevicePixels="False">
      <Path Canvas.Top="1" Data="M0,0 L2.5,2.5 L5,0" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallLeftArrowGlyphTemplateKey}">
    <Canvas Width="4" Height="5" SnapsToDevicePixels="False">
      <Path Data="M3,0 L0.5,2.5 L3,5" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallMinusGlyphTemplateKey}">
    <Canvas Width="9" Height="9">
      <Path StrokeThickness="1" Data="M0,4.5 L9,4.5" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallPlusGlyphTemplateKey}">
    <Canvas Width="9" Height="9">
      <Path StrokeThickness="1" Data="M0,4.5 L9,4.5 M4.5,0 L4.5,9" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallRightArrowGlyphTemplateKey}">
    <Canvas Width="4" Height="5" SnapsToDevicePixels="False">
      <Path Canvas.Left="1" Data="M0,0 L2.5,2.5 L0,5" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallUpArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="4" SnapsToDevicePixels="False">
      <Path Data="M0,3 L2.5,0.5 L5,3" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.ToolBarOverflowDropDownArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="7">
      <Path Canvas.Top="1" Data="M0,0 L5,0 L5,1 L0,1" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
      <Path Canvas.Top="4" Data="M0,0 L2.5,2.5 L5,0" SnapsToDevicePixels="False" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.TreeItemCollapsedGlyphTemplateKey}">
    <Canvas Width="4" Height="8" SnapsToDevicePixels="False">
      <Path Canvas.Top="1" Data="M0,0 L3.5,3.5 L0,7" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.TreeItemExpandedGlyphTemplateKey}">
    <Canvas Width="8" Height="6" SnapsToDevicePixels="False">
      <Path Canvas.Top="2" Canvas.Left="1" Data="M0,0 L3.5,3.5 L7,0" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.UnpinGlyphTemplateKey}">
    <Canvas Width="7" Height="10">
      <Path Data="F1 M0,6 L1,6 L1,0 L6,0 L6,6 L4,6 L4,1 L2,1 L2,6 L7,6 L7,7 L4,7 L4,10 L3,10 L3,7 L0,7" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.UpArrowGlyphTemplateKey}">
    <Canvas Width="7" Height="5" SnapsToDevicePixels="False">
      <Path Data="M0,4 L3.5,0.5 L7,4" StrokeThickness="{StaticResource GlyphStrokeThickness}" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
</ResourceDictionary>