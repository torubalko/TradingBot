<?xml version="1.0" encoding="utf-8"?>
<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" xmlns:themes="clr-namespace:ActiproSoftware.Windows.Themes">
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.DownArrowGlyphTemplateKey}">
    <Canvas Width="7" Height="5">
      <Path Canvas.Top="1" Data="M0,0 L3.5,4 L7,0" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.DropDownArrowGlyphTemplateKey}">
    <Canvas Width="7" Height="5">
      <Path Canvas.Top="1" Data="M0,0 L3.5,4 L7,0" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LargeDownArrowGlyphTemplateKey}">
    <Canvas Width="9" Height="5">
      <Path Data="M0,0 L4.5,5 L9,0" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LargeLeftArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="9">
      <Path Data="M5,0 L0,4.5 L5,9" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LargeRightArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="9">
      <Path Data="M0,0 L5,4.5 L0,9" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LargeUpArrowGlyphTemplateKey}">
    <Canvas Width="9" Height="5">
      <Path Data="M0,5 L4.5,0 L9,5" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.LeftArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="7">
      <Path Data="M4,0 L0,3.5 L4,7" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.MenuPopupGlyphTemplateKey}">
    <Canvas Width="5" Height="7">
      <Path Canvas.Left="1" Data="M0,0 L4,3.5 L0,7" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.OverflowDropDownArrowGlyphTemplateKey}">
    <Canvas Width="7" Height="7">
      <Path Data="M0,0 L7,0 L7,1 L0,1 M0,3 L3.5,7 L7,3" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.OverflowRightArrowGlyphTemplateKey}">
    <Canvas Width="10" Height="7">
      <Path Data="M0,0 L3.5,3.5 L0,7 M5,0 L8.5,3.5 L5,7" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.RightArrowGlyphTemplateKey}">
    <Canvas Width="5" Height="7">
      <Path Canvas.Left="1" Data="M0,0 L4,3.5 L0,7" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallDownArrowGlyphTemplateKey}">
    <Canvas Width="6" Height="4" SnapsToDevicePixels="False">
      <Path Canvas.Top="1" Data="M0,0 L3,3 L6,0" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallDropDownArrowGlyphTemplateKey}">
    <Canvas Width="6" Height="4" SnapsToDevicePixels="False">
      <Path Canvas.Top="1" Data="M0,0 L3,3 L6,0" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallLeftArrowGlyphTemplateKey}">
    <Canvas Width="4" Height="6" SnapsToDevicePixels="False">
      <Path Data="M3,0 L0,3 L3,6" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallRightArrowGlyphTemplateKey}">
    <Canvas Width="4" Height="6" SnapsToDevicePixels="False">
      <Path Canvas.Left="1" Data="M0,0 L3,3 L0,6" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.SmallUpArrowGlyphTemplateKey}">
    <Canvas Width="6" Height="4" SnapsToDevicePixels="False">
      <Path Data="M0,3 L3,0 L6,3" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.ToolBarOverflowDropDownArrowGlyphTemplateKey}">
    <Canvas Width="6" Height="7">
      <Path Canvas.Top="1" Data="M0,0 L6,0 L6,1 L0,1" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
      <Path Canvas.Top="4" Data="M0,0 L3,3 L6,0" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.TreeItemCollapsedGlyphTemplateKey}">
    <Path Name="path" Width="10" Height="10" StrokeThickness="1" Data="M4.5,1.5 L4.5,9.5 L8.5,5.5" SnapsToDevicePixels="False" Stroke="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.TreeItemExpandedGlyphTemplateKey}">
    <Path Name="path" Width="10" Height="10" Data="M3,8 L9,8 L9,2" SnapsToDevicePixels="False" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
  </DataTemplate>
  <DataTemplate x:Key="{x:Static themes:SharedResourceKeys.UpArrowGlyphTemplateKey}">
    <Canvas Width="7" Height="5">
      <Path Data="M0,4 L3.5,0 L7,4" Fill="{Binding Path=(TextElement.Foreground), RelativeSource={RelativeSource Self}}" />
    </Canvas>
  </DataTemplate>
</ResourceDictionary>