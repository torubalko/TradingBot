  <ControlTemplate x:Key="EditorSearchViewExpanderTemplate" TargetType="{x:Type shared:AnimatedExpander}">
    <FrameworkTemplate.Resources>
      <ResourceDictionary>
        <BorderGapMaskConverter x:Key="BorderGapMaskConverter" />
      </ResourceDictionary>
    </FrameworkTemplate.Resources>
    <Grid SnapsToDevicePixels="True">
      <Grid.ColumnDefinitions>
        <ColumnDefinition Width="6" />
        <ColumnDefinition Width="Auto" />
        <ColumnDefinition Width="*" />
        <ColumnDefinition Width="6" />
      </Grid.ColumnDefinitions>
      <Grid.RowDefinitions>
        <RowDefinition Height="Auto" />
        <RowDefinition Height="Auto" />
        <RowDefinition Height="*" />
        <RowDefinition Height="6" />
      </Grid.RowDefinitions>
      <Border Grid.Row="1" Grid.ColumnSpan="4" Grid.RowSpan="3" BorderThickness="0,1,0,0" BorderBrush="#00FFFFFF" Background="{TemplateBinding Control.Background}" />
      <Border Grid.Row="1" Grid.ColumnSpan="4" Grid.RowSpan="3" BorderThickness="1" CornerRadius="4" BorderBrush="{DynamicResource {x:Static themes:AssetResourceKeys.GroupBoxHighlightNormalBrushKey}}">
        <UIElement.OpacityMask>
          <MultiBinding ConverterParameter="7" Converter="{StaticResource BorderGapMaskConverter}">
            <Binding ElementName="Header" Path="ActualWidth" />
            <Binding Path="ActualWidth" RelativeSource="{RelativeSource Self}" />
            <Binding Path="ActualHeight" RelativeSource="{RelativeSource Self}" />
          </MultiBinding>
        </UIElement.OpacityMask>
        <Border Name="outerBorder" BorderThickness="0,1,0,0" BorderBrush="{TemplateBinding Border.BorderBrush}">
          <Border Name="innerBorder" BorderThickness="0,1,0,0" BorderBrush="{DynamicResource {x:Static themes:AssetResourceKeys.GroupBoxHighlightNormalBrushKey}}" />
        </Border>
      </Border>
      <Border Name="Header" Padding="3,1,3,0" Grid.Column="1" Grid.Row="0" Grid.RowSpan="2">
        <StackPanel Orientation="Horizontal">
          <Button Name="headerButton" MinWidth="18" MinHeight="18" Content="+" Padding="0" VerticalAlignment="Center" Command="{Binding ToggleOptionsPanelExpandedCommand}" />
          <Button Margin="3,0,0,0" Padding="0" Content="{TemplateBinding HeaderedContentControl.Header}" IsTabStop="False" Focusable="False" SnapsToDevicePixels="{TemplateBinding UIElement.SnapsToDevicePixels}" VerticalAlignment="Center" Command="{Binding ToggleOptionsPanelExpandedCommand}">
            <Control.Template>
              <ControlTemplate TargetType="{x:Type Button}">
                <ContentPresenter RecognizesAccessKey="True" />
              </ControlTemplate>
            </Control.Template>
          </Button>
        </StackPanel>
      </Border>
      <shared:AnimatedExpanderDecorator Name="content" Grid.Column="1" Grid.Row="2" Grid.ColumnSpan="2" ExpandDuration="{TemplateBinding shared:AnimatedExpander.ExpandDuration}" CollapseDuration="{TemplateBinding shared:AnimatedExpander.CollapseDuration}">
        <ContentPresenter Content="{TemplateBinding ContentControl.Content}" ContentTemplate="{TemplateBinding ContentControl.ContentTemplate}" Margin="{TemplateBinding Control.Padding}" SnapsToDevicePixels="{TemplateBinding UIElement.SnapsToDevicePixels}" />
      </shared:AnimatedExpanderDecorator>
    </Grid>
    <ControlTemplate.Triggers>
      <Trigger Property="Expander.IsExpanded" Value="True">
        <Setter TargetName="outerBorder" Property="Border.CornerRadius" Value="3" />
        <Setter TargetName="outerBorder" Property="Border.BorderThickness" Value="1" />
        <Setter TargetName="innerBorder" Property="Border.CornerRadius" Value="2" />
        <Setter TargetName="innerBorder" Property="Border.BorderThickness" Value="1" />
        <Setter TargetName="headerButton" Property="ContentControl.Content" Value="-" />
        <Setter TargetName="content" Property="shared:AnimatedExpanderDecorator.IsExpanded" Value="True" />
      </Trigger>
    </ControlTemplate.Triggers>
  </ControlTemplate>
  <Style x:Key="{x:Type editorPrimitives:EditorSearchView}" TargetType="{x:Type editorPrimitives:EditorSearchView}">
